<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 

<bean id = "addre" class="Address">
<property name="city" value="Ghaziabad"></property>
<property name="country" value="India"></property>
</bean>

<bean id = "ddre" class="Address">
<property name="city" value="Moradabad"></property>
<property name="country" value="India"></property>
</bean>

<bean id = "address" class="Address">
<property name="city" value="Delhi"></property>
<property name="country" value="India"></property>
</bean>

<bean id = "empbean" class="Employee" autowire="byName">
<property name="name" value = "Rohan Gupta"></property>

</bean>

</beans>